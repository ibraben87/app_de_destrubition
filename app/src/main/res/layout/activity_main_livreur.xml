        android:textColor="#1C1061"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@id/Commandes_en_attente"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@id/Commandes_en_attente"
        app:layout_constraintTop_toBottomOf="@id/Commandes_en_attente" />

    <ImageView
        android:id="@+id/imageView6"
